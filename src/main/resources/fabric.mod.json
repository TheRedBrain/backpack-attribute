{
	"schemaVersion": 1,
	"id": "backpackattribute",
	"version": "${version}",
	"name": "Backpack Attribute",
	"description": "An unconventional backpack mod using an entity attribute.",
	"authors": [
		"TheRedBrain"
	],
	"contact": {
		"sources": "https://github.com/TheRedBrain/backpack-attribute"
	},
	"license": "MIT",
	"icon": "assets/backpackattribute/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.github.theredbrain.backpackattribute.BackpackAttribute"
		],
		"client": [
			"com.github.theredbrain.backpackattribute.BackpackAttributeClient"
		],
		"modmenu": [
			"com.github.theredbrain.backpackattribute.compatibility.ModMenuCompatibility"
		]
	},
	"mixins": [
		"backpackattribute.mixins.json"
	],
	"depends": {
		"minecraft": "${minecraft_version}",
		"fabricloader": ">=${loader_version}",
		"fabric-api": "*",
		"slotcustomizationapi": "${slot_customization_api_version}",
		"cloth-config": ">=${cloth_config_version}"
	},
	"suggests": {
		"inventorysizeattributes": ">=${inventory_size_attributes_version}",
		"modmenu": ">=${mod_menu_version}"
	}
}